CREATE MEMORY TABLE PUBLIC.SUCURSAL(ID INTEGER NOT NULL PRIMARY KEY,DIRECCION CHARACTER(25) NOT NULL)
CREATE MEMORY TABLE PUBLIC.CLIENTE(DNI CHARACTER(10) NOT NULL PRIMARY KEY,NOMBREAPELLIDOS CHARACTER(20) NOT NULL,DIRECCION CHARACTER(20) NOT NULL,POBLACION CHARACTER(20) NOT NULL,CODPOSTAL CHARACTER(5) NOT NULL,FECHACARNETCONDUCIR TIMESTAMP NOT NULL,DIGITOSTC CHARACTER(16) NOT NULL,MESTC NUMERIC(2) NOT NULL,"a\u00f1oTC" NUMERIC(4) NOT NULL,CVCTC NUMERIC(3) NOT NULL,TIPOTC CHARACTER(10) NOT NULL)
CREATE MEMORY TABLE PUBLIC.CATEGORIA(NOMBRE CHARACTER(20) NOT NULL PRIMARY KEY,PRECIOMODILIMITADA DOUBLE NOT NULL,PRECIOMODKMS DOUBLE NOT NULL,PRECIOSEGUROTRIESGO DOUBLE NOT NULL,PRECIOSEGUROTERCEROS DOUBLE NOT NULL,CATEGORIASUPERIOR CHARACTER(10),PRECIOKMMODKMS DOUBLE NOT NULL,CONSTRAINT FK_CATEGORIA_1 FOREIGN KEY(CATEGORIASUPERIOR) REFERENCES PUBLIC.CATEGORIA(NOMBRE))
CREATE MEMORY TABLE PUBLIC.EMPLEADO(DNI CHARACTER(10) NOT NULL PRIMARY KEY,NOMBRE CHARACTER(25) NOT NULL,ADMINISTRADOR BOOLEAN NOT NULL,SUCURSAL INTEGER NOT NULL,CONSTRAINT FK_EMPLEADO_1 FOREIGN KEY(SUCURSAL) REFERENCES PUBLIC.SUCURSAL(ID))
CREATE MEMORY TABLE PUBLIC.RESERVA(ID INTEGER NOT NULL PRIMARY KEY,FECHARECOGIDA TIMESTAMP NOT NULL,FECHADEVOLUCION TIMESTAMP NOT NULL,MODALIDADALQUILER CHARACTER(10) NOT NULL,CATEGORIA CHARACTER(20) NOT NULL,CLIENTEREALIZA CHARACTER(10) NOT NULL,SUCURSALRECOGIDA INTEGER NOT NULL,SUCURSALDEVOLUCION INTEGER NOT NULL,CONSTRAINT FK_TABLARESERVA_1 FOREIGN KEY(CLIENTEREALIZA) REFERENCES PUBLIC.CLIENTE(DNI),CONSTRAINT FK_TABLARESERVA_4 FOREIGN KEY(SUCURSALRECOGIDA) REFERENCES PUBLIC.SUCURSAL(ID),CONSTRAINT FK_TABLARESERVA_5 FOREIGN KEY(SUCURSALDEVOLUCION) REFERENCES PUBLIC.SUCURSAL(ID),CONSTRAINT FK_TABLARESERVA_2 FOREIGN KEY(CATEGORIA) REFERENCES PUBLIC.CATEGORIA(NOMBRE))
CREATE MEMORY TABLE PUBLIC.COCHE(MATRICULA CHARACTER(10) NOT NULL PRIMARY KEY,KMSACTUALES DOUBLE NOT NULL,SUCURSAL INTEGER,CATEGORIA CHARACTER(10) NOT NULL,NOMBRE CHARACTER(20) NOT NULL,CONSTRAINT FK_TABLACOCHE_1 FOREIGN KEY(SUCURSAL) REFERENCES PUBLIC.SUCURSAL(ID),CONSTRAINT FK_TABLACOCHE_2 FOREIGN KEY(NOMBRE) REFERENCES PUBLIC.CATEGORIA(NOMBRE))
CREATE MEMORY TABLE PUBLIC.ENTREGA(ID INTEGER NOT NULL PRIMARY KEY,FECHA TIMESTAMP NOT NULL,TIPOSEGURO CHARACTER(10) NOT NULL,KMS DOUBLE NOT NULL,COMBUSTIBLE DOUBLE NOT NULL,COCHEASIGNADO CHARACTER(10) NOT NULL,EMPLEADOREALIZA CHARACTER(10) NOT NULL,CONSTRAINT FK_TABLAENTREGA_1 FOREIGN KEY(ID) REFERENCES PUBLIC.RESERVA(ID),CONSTRAINT FK_TABLAENTREGA_3 FOREIGN KEY(EMPLEADOREALIZA) REFERENCES PUBLIC.EMPLEADO(DNI),CONSTRAINT FK_TABLAENTREGA_2 FOREIGN KEY(COCHEASIGNADO) REFERENCES PUBLIC.COCHE(MATRICULA))
CREATE MEMORY TABLE PUBLIC.DEVOLUCION(ID INTEGER NOT NULL PRIMARY KEY,FECHA TIMESTAMP NOT NULL,TOTALACOBRAR DOUBLE NOT NULL,COBRADO BOOLEAN NOT NULL,KMS DOUBLE NOT NULL,COMBUSTIBLE DOUBLE NOT NULL,EMPLEADOREALIZA CHARACTER(10) NOT NULL,CONSTRAINT FK_TABLADEVOLUCION_1 FOREIGN KEY(ID) REFERENCES PUBLIC.ENTREGA(ID),CONSTRAINT FK_TABLADEVOLUCION_2 FOREIGN KEY(EMPLEADOREALIZA) REFERENCES PUBLIC.EMPLEADO(DNI))
CREATE MEMORY TABLE PUBLIC."Da\u00f1o"(ID INTEGER,ZONA CHARACTER(10) NOT NULL,DESCRIPCION VARCHAR(50) NOT NULL,ENTREGA INTEGER,DEVOLUCION INTEGER,CONSTRAINT "FK_TablaDa\u00f1o_1" FOREIGN KEY(ENTREGA) REFERENCES PUBLIC.ENTREGA(ID),CONSTRAINT "FK_TablaDa\u00f1o_2" FOREIGN KEY(DEVOLUCION) REFERENCES PUBLIC.DEVOLUCION(ID))